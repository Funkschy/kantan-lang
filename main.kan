import "lexer"
import "io"
import "map"

def main(): void {
    let l = lexer.create("1337 42test");

    let t = lexer.Token { ty: 0, len: 0, lexeme: null };
    while (t = lexer.next_token(&l)).ty != lexer.eof() {
        io.printf("%d: %.*s\n", t.ty, t.len, t.lexeme);
    }

    io.printf("Read %d byte\n", l.current - l.source);
}
