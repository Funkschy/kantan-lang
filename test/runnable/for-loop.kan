import "io";

def main(): void {
    let i = 100;
    for let i = 0; i < 3; i += 1 {
        io.printf("%d", i+1);
    }
    io.printf("\n");
    for let i = 3; i > 0; i -= 1 {
        io.printf("%d", i);
    }
    io.printf("\n");
    for ;i < 101; {
        io.printf("%d\n", i);
        i = i + 1;
    }
}
